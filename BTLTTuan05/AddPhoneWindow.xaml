<Window x:Class="BTLTTuan05.AddPhoneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BTLTTuan05"
        mc:Ignorable="d"
        Title="Nhập số điện thoại" Height="450" Width="280">
    <Canvas>
        <Label Content="Nhập số điện thoại" Canvas.Left="10" Canvas.Top="10" Width="120" Height="30"/>
        <TextBox x:Name="txtPhone" Canvas.Left="10" Canvas.Top="50" Width="250" Height="30"/>
        <Button x:Name="btnOK" Content="OK" Canvas.Left="10" Canvas.Top="100" Width="120" Height="30" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" Canvas.Left="140" Canvas.Top="100" Width="120" Height="30" Click="btnCancel_Click"/>
    </Canvas>
</Window>
